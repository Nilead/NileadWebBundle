<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="nilead.frontend.checkout.checkout" path="/">
        <default key="_controller">NileadCoreBundle:Checkout/Checkout:checkout</default>
    </route>

    <route id="nilead.frontend.checkout.index" path="/">
        <default key="_controller">NileadCoreBundle:Checkout/Checkout:checkout</default>
    </route>

    <route id="nilead.frontend.checkout.callback" path="/callback">
        <default key="_controller">NileadCoreBundle:Checkout/Checkout:callback</default>
    </route>
</routes>